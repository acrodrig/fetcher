{
  "name": "@acr/fetcher",
  "license": "MIT",
  "exports": "./mod.ts",
  "fmt": {
    "lineWidth": 180,
    "include": [
      "src/",
      "test/"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/encoding": "jsr:@std/encoding@^1.0.10"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "mod.ts",
      "src/**/*.ts"
    ]
  },
  "tasks": {
    "check": "deno check **/*.ts && deno lint && deno fmt --check",
    "lint": "deno lint src test",
    "release": "release",
    "test": "deno test -A"
  }
}
